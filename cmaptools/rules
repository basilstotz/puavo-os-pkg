#!/bin/sh

set -eu

command=$1
shift

case "${command}" in
    configure)
        upstream_dir=$1

        mkdir -p /usr/bin
        ln -fns -T "${upstream_dir}/bin/CmapTools" /usr/bin/CmapTools

