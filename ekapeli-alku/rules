case "${command}" in
    configure)
	upstream_dir=$1

	mkdir -p /opt/ekapeli-alku
	ln -fns "${upstream_dir}/Ekapeli-Alku.x86" \
		/opt/ekapeli-alku/Ekapeli-Alku.x86
	ln -fns "${upstream_dir}/Ekapeli-Alku_Data" \
		/opt/ekapeli-alku/Ekapeli-Alku_Data
	ln -fns "${upstream_dir}/ekapeli_alku_title.png" \
		/opt/ekapeli-alku/ekapeli_alku_title.png

	cat <<'EOF' > /usr/share/applications/ekapeli-alku.desktop
[Desktop Entry]
Name=Ekapeli Alku
Exec=/opt/ekapeli-alku/Ekapeli-Alku.x86
Type=Application
Terminal=false
Icon=/opt/ekapeli-alku/ekapeli_alku_title.png
StartupNotify=true
Categories=Education;Games;
EOF
	;;
    unconfigure)
	rm -rf /opt/ekapeli-alku /usr/share/applications/ekapeli-alku.desktop
	;;
    unpack)
	upstream_pack=$1
	upstream_dir=$2
	unzip -q -d "${upstream_dir}" "${upstream_pack}"
	chmod 755 "${upstream_dir}/Ekapeli-Alku.x86"
	extract_icon "$upstream_dir"
	;;
    *)
	;;
esac
